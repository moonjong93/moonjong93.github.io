person.name = 'moon jong min';
console.log(person.name); // moon jong min

const person: IPerson = new Person('Mark', 35);
person.getName();

// interfaceë¥¼ í™œìš© í•˜ë©´ ì´ëŸ°ì‹ìœ¼ë¡œë„ ì‚¬ìš© ê°€ëŠ¥í•¨
